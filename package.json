{
	"name": "template",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"codegen": "npm run codegen:migrations && npm run codegen:types",
		"codegen:migrations": "supabase db diff -f $RANDOM",
		"codegen:types": "supabase gen types typescript --local --schema public > ./src/lib/supabase/types.ts",
		"build": "vite build",
		"preview": "vite preview",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "npm run test:integration && npm run test:unit",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@melt-ui/pp": "^0.1.4",
		"@melt-ui/svelte": "^0.65.2",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.30.0",
		"postcss": "^8.4.32",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"supabase": "^1.123.4",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-sequential-preprocessor": "^2.0.1",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^0.32.2"
	},
	"dependencies": {
		"@supabase/ssr": "^0.0.10",
		"@supabase/supabase-js": "^2.39.0"
	}
}
